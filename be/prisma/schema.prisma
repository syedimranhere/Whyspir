generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  pfp      String @default("https://avatars.githubusercontent.com/u/100484?v=4")
}
